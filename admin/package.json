{
  "private": true,
  "author": "elliot <li.shanlue@intellif.com>",
  "scripts": {
    "dev": "max dev",
    "build": "max build",
    "format": "oxfmt . --write",
    "format:check": "oxfmt . --check",
    "lint": "oxlint . && stylelint '**/*.{css,less,scss,pcss}'",
    "lint:fix": "oxlint . --fix && stylelint '**/*.{css,less,scss,pcss}' --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "postinstall": "max setup",
    "setup": "max setup",
    "start": "npm run dev"
  },
  "dependencies": {
    "@alova/adapter-axios": "^2.0.17",
    "@ant-design/icons": "^6.1.0",
    "@ant-design/pro-components": "^2.8.10",
    "@types/qs": "^6.14.0",
    "@umijs/max": "^4.6.20",
    "alova": "^3.4.1",
    "antd": "^6.1.3",
    "axios": "^1.13.2",
    "qs": "^6.14.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "jsdom": "^27.4.0",
    "lint-staged": "^13.2.0",
    "oxfmt": "^0.23.0",
    "oxlint": "^1.38.0",
    "postcss": "^8.5.6",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-tailwindcss": "^1.0.1",
    "stylelint-order": "^7.0.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "oxfmt --write",
      "oxlint --fix"
    ],
    "*.{css,less,scss,pcss}": [
      "stylelint --fix"
    ]
  }
}